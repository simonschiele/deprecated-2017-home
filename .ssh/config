Host *.psaux.de psaux.de
  User simon

Host *.nerdmail.de nerdmail.de
  User simon

Host enkheim.*
  Port 2222
  User simon

Host tuxcode.org
  Port 11167
  User git

Host *.github.com github.com f1-gitserv.*
  User git

Host *
	SendEnv LANG LC_* TERM COLORTERM COLORCOUNT
	ServerAliveInterval = 15
	ServerAliveCountMax =  10

	# SSH Agent Forwarding
	ForwardAgent yes 

	# Session 
	#ControlMaster auto
	ControlPath ~/.ssh/tmp/%r@%h:%p

	# Automatically add to knownhosts
	# ask (default) | no (auto add) | yes (deny)
	#StrictHostKeyChecking no

