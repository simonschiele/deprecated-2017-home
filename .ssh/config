Host *.psaux.de *.nerdmail.de psaux.de secure enkheim nerdmail
    User simon

Host enkheim.* enkheim neukoelln.* neukoelln
    User simon
    Port 2222

Host *.github.com github.com f1-gitserv.* git.* gitlab.*
    User git

Host tuxcode.org
    User git
    Port 11167

Host 192.168.178.23 mediacenter
    User root

Host *+*
    ProxyCommand ssh $(echo %h | sed 's/+[^+]*$//;s/\([^+%%]*\)%%\([^+]*\)$/\2 -l \1/;s/:/ -p /') exec nc -w1 $(echo %h | sed 's/^.*+//;/:/!s/$/ %p/;s/:/ /')

Host *
    SendEnv LANG LC_* GIT_* TERM COLORTERM COLORCOUNT
    Protocol 2
    Compression yes
    TCPKeepAlive yes
    ServerAliveInterval = 20
    ServerAliveCountMax = 10

    # SSH Agent Forwarding
    ForwardAgent yes

    # Session multiplexing
    #ControlMaster auto
    #ControlPath ~/.ssh/tmp/%r@%h:%p
    #ControlPersist 10m # 4h | 1800

    # Automatically add to knownhosts
    # ask (default) | no (auto add) | yes (deny)
    #StrictHostKeyChecking no
